{
    "mcpServers": {
        "web-search": {
            "command": "python",
            "args": [
                "${MCP_PROJECT_DIR}/scout/my_mcp/local_servers/web_search.py"
            ],
            "transport": "stdio"
        },
        "dataflow": {
            "command": "python",
            "args": [
                "${MCP_PROJECT_DIR}/scout/my_mcp/local_servers/dataflow.py"
            ],
            "transport": "stdio"
        },
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "${MCP_FILESYSTEM_DIR}"
            ]
        },
        "git": {
            "command": "uvx",
            "args": [
                "mcp-server-git"
            ],
            "transport": "stdio"
        },
        "github": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "-e",
                "GITHUB_PERSONAL_ACCESS_TOKEN",
                "ghcr.io/github/github-mcp-server"
            ],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "${MCP_GITHUB_PAT}"
            }
        },
        "supabase": {
            "command": "npx",
            "args": [
                "-y",
                "@supabase/mcp-server-supabase@latest",
                "--access-token",
                "${MCP_SUPABASE_PAT}"
            ]
        }
    }
}